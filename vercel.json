{
    "builds": [{
        "src": "config/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb",
                    "requirements": "requirements.txt",
                    "pythonVersion": "3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ]
}